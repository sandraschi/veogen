      "id": 3,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": false,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "veogen-logs"
          },
          "expr": "{job=\"veogen-backend\"} |= \"ERROR\"",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "loki",
            "uid": "veogen-logs"
          },
          "expr": "{job=\"veogen-backend\"} |= \"CRITICAL\"",
          "refId": "B"
        }
      ],
      "title": "Error Logs",
      "type": "logs"
    }
